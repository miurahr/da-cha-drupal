msgid "taxonomy"
msgstr "タクソノミー"

#: modules/taxonomy/taxonomy.module:418
msgid "- None selected -"
msgstr "- 選択されていません -"

#: modules/taxonomy/taxonomy.module:421
msgid "- None -"
msgstr "- なし -"

